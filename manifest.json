{
    "manifest_version": 3,
    "name": "Bookmark Tab",
    "description": "Displays your bookmarks in a neat layout on the new tab page.",
    "icons": {
      "256": "assets/logo.png"
    },
    "version": "1.0",
    "chrome_url_overrides" : {
        "newtab": "index.html"
      },
    "permissions": ["bookmarks", "storage", "alarms"],
    "options_page": "options.html",
    "default_locale": "en",
    "background": {
      "service_worker": "weather_background.js"
    }
}
